                PuntajesPuesto[PuntajesPuesto.length] = parseFloat("@item.me_prueba.reporte_conocimiento.nu_porcentaje")*-1;
                PuntajesCandidato[PuntajesCandidato.length] = parseFloat("@item.me_prueba.reporte_conocimiento.nu_porcentajes_candidatos");
            </script>
        }
    }
}

