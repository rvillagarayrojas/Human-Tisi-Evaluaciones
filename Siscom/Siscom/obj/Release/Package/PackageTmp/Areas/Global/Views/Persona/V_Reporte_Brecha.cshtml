        Valor_C = parseFloat("@Model.ls_disc[0].me_prueba.reporte_conocimiento.nu_porcentajes_candidatos");

        Valor_DP = parseFloat("@Model.ls_disc[12].me_prueba.reporte_conocimiento.nu_porcentaje") * -1;
        Valor_IP = parseFloat("@Model.ls_disc[8].me_prueba.reporte_conocimiento.nu_porcentaje") * -1;
        Valor_SP = parseFloat("@Model.ls_disc[4].me_prueba.reporte_conocimiento.nu_porcentaje") * -1;
        Valor_CP = parseFloat("@Model.ls_disc[0].me_prueba.reporte_conocimiento.nu_porcentaje") * -1;
    </script>
    foreach (var item in Model.ls_disc)
    {
        if (item.me_prueba.reporte_conocimiento.vc_letra_parametro != vc_letra)
        {vc_letra = item.me_prueba.reporte_conocimiento.vc_letra_parametro;
            <script type="text/javascript">
                suma = suma + parseFloat("@item.me_prueba.reporte_conocimiento.nu_porcentaje_adaptacion");
