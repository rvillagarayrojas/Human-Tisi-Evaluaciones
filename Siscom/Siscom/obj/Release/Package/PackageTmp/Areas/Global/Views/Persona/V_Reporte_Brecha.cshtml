<script type="text/javascript" src="~/Scripts/PrintArea.js"></script>
<script type="text/javascript">

    $(document).ready(function () {

        $("#btnImprimir").click(function () {
            $('#Contenido').printArea();
        });

        CambiarColores(); BuscarAttr2();
        var f = new Date();
        $('#Fecha').html(("0" + f.getDate()).slice(-2) + "/" + ("0" + (f.getMonth() + 1)).slice(-2) + "/" + f.getFullYear());
        $(".highcharts-button").remove();
        $(".highcharts-legend-item").remove();
    });

    function BuscarAttr2() {

        cronometro = setInterval
                     (
                        function () {
                            $('#container5 .highcharts-tracker').removeAttr('onmouseover');
                            $('#container6 .highcharts-tracker').removeAttr('onmouseover');
                            $('#container7 .highcharts-tracker').removeAttr('onmouseover');
                            $('#container8 .highcharts-tracker').removeAttr('onmouseover');

                            $('#container5 .highcharts-tracker rect').each(function () {
                                $(this).attr('fill', '#2c3491');
                            });
                            $('#container6 .highcharts-tracker rect').each(function () {
                                $(this).attr('fill', '#27833a');
                            });
                            $('#container7 .highcharts-tracker rect').each(function () {
                                $(this).attr('fill', '#e8b821');
                            });
                            $('#container8 .highcharts-tracker rect').each(function () {
                                $(this).attr('fill', '#da1b51');
                            });
                            }
                        )
    }


    function CambiarColores() {
        var cont = 1;
        var cont_2 = 1;
        $('#container4 .highcharts-tracker').each(function () {
            $(this).removeAttr('onmouseover');
            cont_2 = 1;
            if (cont == 1) {
                $('#container4 .highcharts-tracker rect').each(function () {
                    if (cont_2 < 5) {
                        if (cont_2 == 1) {
                            $(this).attr('fill', '#da1b51');
                        }
                        if (cont_2 == 2) {
                            $(this).attr('fill', '#e8b821');
                        }
                        if (cont_2 == 3) {
                            $(this).attr('fill', '#27833a');
                        }
                        if (cont_2 == 4) {
                            $(this).attr('fill', '#2c3491');
                        }
                    }
                    cont_2++;
                });
            }
            if (cont == 2) {
                $('#container4 .highcharts-tracker rect').each(function () {
                    if (cont_2 > 4) {
                        if (cont_2 == 5) {
                            $(this).attr('fill', '#da1b51');
                        }
                        if (cont_2 == 6) {
                            $(this).attr('fill', '#e8b821');
                        }
                        if (cont_2 == 7) {
                            $(this).attr('fill', '#27833a');
                        }
                        if (cont_2 == 8) {
                            $(this).attr('fill', '#2c3491');
                        }
                    }
                    cont_2++;
                });
            }
            cont++;

        });

    }

</script>
