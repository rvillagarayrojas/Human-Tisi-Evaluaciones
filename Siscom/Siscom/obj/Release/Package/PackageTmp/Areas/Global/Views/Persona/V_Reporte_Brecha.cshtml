@if (Model.ls_disc != null && Model.ls_disc.Count > 0)
{ 
    <script type="text/javascript">
        Valor_D = parseFloat("@Model.ls_disc[12].me_prueba.reporte_conocimiento.nu_porcentajes_candidatos");
